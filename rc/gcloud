# The next line updates PATH for the Google Cloud SDK.
if [ -f "$HOME/bin/google-cloud-sdk/path.$_dotfiles_current_shell.inc" ]; then
    source "$HOME/bin/google-cloud-sdk/path.$_dotfiles_current_shell.inc"
fi

# The next line enables shell command completion for gcloud.
if [ -f "$HOME/bin/google-cloud-sdk/completion.$_dotfiles_current_shell.inc" ]; then
    source "$HOME/bin/google-cloud-sdk/completion.$_dotfiles_current_shell.inc"
fi
